<div class="demand-card-wrapper col-xs-12 col-sm-4 col-md-3">
  <div class="demand-card panel panel-default bootcards-file">

    <div class="panel-heading">
      <a href="#">
        <i class="icon-file-pdf"></i>
      </a>
      <h4 class="list-group-item-heading">
        <a href="#"><%=intervention.nature%></a>
      </h4>
    </div>
    <div class="list-group">
      <div class="list-group-item">
        <%= render partial: 'shared/client_details',
                    locals: {client: intervention.client} %>


        <strong>Date</strong><br>
        <div class ="date-demand-card">
        <div>
            Vendredi 21 octobre<br>
            15h30 - 17h00<br>
        </div>
          <div class= "calendar-date-demand-card">
            <i class="fa fa-calendar" aria-hidden="true"></i>
          </div>
        </div>


      </div>
    </div>
    <div class="panel-footer clearfix">
      <div class="btn-group btn-group-justified">
        <div class="btn-group">
          <button class="btn btn-default">
            <i class="fa fa-check"></i>
          </button>
        </div>
        <div class="btn-group">
          <button class="btn btn-default">
            <i class="fa fa-remove  "></i>
          </button>
        </div>
      </div>
    </div>
  </div>
</div>
